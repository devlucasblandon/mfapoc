# Variables de Entorno para Producción AWS
# IMPORTANTE: Cambiar estos valores en producción

# Base de Datos
DB_PASSWORD=your_secure_db_password_here
DATABASE_URL=postgresql://poc3_user:your_secure_db_password_here@postgres:5432/poc3_db

# JWT y Encriptación
JWT_SECRET_KEY=your_super_secure_jwt_secret_key_here_change_in_production
ENCRYPTION_KEY=your_32_character_encryption_key_here

# Redis
REDIS_URL=redis://redis:6379/0

# Grafana
GRAFANA_PASSWORD=your_grafana_admin_password_here

# AWS (se configurarán automáticamente)
AWS_REGION=us-east-1
AWS_ACCOUNT_ID=your_aws_account_id

# Aplicación
ENVIRONMENT=production
LOG_LEVEL=INFO
PORT=8080
